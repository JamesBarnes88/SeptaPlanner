<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.septaplanneralpha"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.septaplanneralpha.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.septaplanneralpha.SeptaActivity"
            android:label="@string/title_activity_septa" >
        </activity>
        <activity
            android:name="com.example.septaplanneralpha.RecentTripsActivity"
            android:label="@string/title_activity_recent_trips" >
        </activity>
        <activity
            android:name="com.example.septaplanneralpha.TripActivity"
            android:label="@string/title_activity_trip" >
        </activity>
        <activity
            android:name="com.example.septaplanneralpha.SavedActivity"
            android:label="@string/title_activity_saved" >
        </activity>
        <activity
            android:name="com.example.septaplanneralpha.TripDisplayActivity"
            android:label="@string/title_activity_trip_display" >
        </activity>
    </application>

</manifest>
